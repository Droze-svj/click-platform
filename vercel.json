{
  "version": 2,
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ],
  "functions": {
    "client/**/*.js": {
      "runtime": "@vercel/next"
    }
  },
  "regions": ["cdg1"],
  "outputDirectory": "client/.next"
}
