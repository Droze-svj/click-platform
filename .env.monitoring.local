# DEVELOPMENT MONITORING CONFIGURATION
# Safe settings for local development

# Analytics (Disabled for development)
NEXT_PUBLIC_GA_MEASUREMENT_ID=
NEXT_PUBLIC_MIXPANEL_TOKEN=
NEXT_PUBLIC_ANALYTICS_ENDPOINT=
NEXT_PUBLIC_DISABLE_PRIVACY_ANALYTICS=true

# Alerting (Console only for development)
SMTP_HOST=
SMTP_PORT=
SMTP_USER=
SMTP_PASS=
ALERT_FROM_EMAIL=
ALERT_TO_EMAILS=
SLACK_BOT_TOKEN=
SLACK_ALERT_CHANNEL=
SLACK_WEBHOOK_URL=
ALERT_WEBHOOK_URL=

# Thresholds (Relaxed for development)
ALERT_COOLDOWN_MINUTES=1
MAX_ALERTS_PER_HOUR=50
PERFORMANCE_RESPONSE_TIME_THRESHOLD=5000
PERFORMANCE_ERROR_RATE_THRESHOLD=0.1
PERFORMANCE_MEMORY_THRESHOLD=0.9
PERFORMANCE_CPU_THRESHOLD=0.8

# Core Web Vitals (Relaxed)
CWV_CLS_THRESHOLD=0.25
CWV_FID_THRESHOLD=300
CWV_FCP_THRESHOLD=3000
CWV_LCP_THRESHOLD=4000
